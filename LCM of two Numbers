# Method 1

num1 = 12
num2 = 14
for i in range(max(num1,num2),1+(num1*num2),max(num1,num2)):
  if i % num1 == i % num2 ==0:
    lcm=i
    break
print(lcm)

# Method 2

num1 = 12
num2 = 14
hcf=1
for i in range(1, min(num1, num2)):
  if num1 % i== num2 % i==0:
    hcf =1
lcm= (num1 * num2 )// hcf
print(lcm)
